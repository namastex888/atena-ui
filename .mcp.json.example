{
  "mcpServers": {
    "automagik-forge": {
      "type": "sse",
      "url": "http://192.168.112.154:8889/sse"
    },
    "ask-repo-agent": {
      "type": "sse",
      "url": "https://mcp.deepwiki.com/sse"
    },
    "search-repo-docs": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"]
    },
    "send_whatsapp_message": {
      "command": "uvx",
      "args": [
        "automagik-tools@0.7.8",
        "tool",
        "evolution-api"
      ],
      "env": {
        "EVOLUTION_API_BASE_URL": "YOUR_EVOLUTION_API_URL",
        "EVOLUTION_API_API_KEY": "YOUR_EVOLUTION_API_KEY",
        "EVOLUTION_API_INSTANCE": "YOUR_INSTANCE_NAME",
        "EVOLUTION_API_FIXED_RECIPIENT": "YOUR_RECIPIENT"
      }
    },
    "postgres": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-postgres",
        "YOUR_DATABASE_CONNECTION_STRING"
      ]
    },
    "automagik-hive": {
      "command": "uvx",
      "args": [
        "automagik-tools@0.8.17",
        "tool",
        "automagik-hive"
      ],
      "env": {
        "HIVE_API_BASE_URL": "YOUR_HIVE_API_URL",
        "HIVE_API_KEY": "YOUR_HIVE_API_KEY",
        "HIVE_TIMEOUT": "300"
      }
    },
    "wait": {
      "command": "uvx",
      "args": [
        "automagik-tools@0.7.8",
        "tool",
        "wait"
      ]
    },
    "zen": {
      "command": "sh",
      "args": [
        "-c",
        "exec $(which uvx || echo uvx) --from git+https://github.com/BeehiveInnovations/zen-mcp-server.git zen-mcp-server"
      ],
      "env": {
        "PATH": "/usr/local/bin:/usr/bin:/bin:/opt/homebrew/bin:~/.local/bin",
        "GEMINI_API_KEY": "YOUR_GEMINI_API_KEY",
        "XAI_API_KEY": "YOUR_XAI_API_KEY"
      }
    }
  }
}